            android:name="com.example.chartdemo.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.chartdemo.XYChartBuilder" />
        <activity android:name="com.example.chartdemo.PieChartBuilder" />
        <activity android:name="org.achartengine.GraphicalActivity" />
    </application>

</manifest>
